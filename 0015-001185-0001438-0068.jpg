	$(function(){	
		
		$(window).on("load", function(){
			if($('img[usemap]').length) $('img[usemap]').rwdImageMaps();				
		});
		
		
		//console.log($(".orbitGallery .orbit-container").length);
		if(layoutMode =="normal"){
			$(".orbitGallery .example-orbit:not(':first')").parent('div').css({left:"-9999px", position:"absolute"});
			$(window).on("load", function(){
				var slideHeight= $(".orbitGallery .orbit-container:first > ul").height();
				$(".orbitGallery .orbit-container > ul").css({height:slideHeight+"px"});
			});
		};
		
		
		/* RefinedArea Show/Hide Logic */
		$(".refinedArea").each(function(){
			if($(this).find("li").length > 0) $(this).show();
		});
		
		//Category Discount Logic
		if($(".orbit-container").length){
			$(".catediscount").each(function(){
				if($.trim($(this).html()) === ""){
				$(this).parent().append($(this));
				}
			});
		}
		
		//Submenu Positioning JS		
		function resNavigation(){
			if($("#navContainer")){
			    if(typeof($("#navContainer").offset())!=="undefined" && typeof($("#navContainer").offset().left)!=="undefined"){
			     var $MainWidth= $("#navContainer").width() + $("#navContainer").offset().left;
			    }else{
			     var $MainWidth= $("#navContainer").width();
			    }
			   }
			$(".subMenu").each(function(){
				var $this= $(this);
				var MenuWidth=0;
				$this.find('ul').each(function(){
					MenuWidth+=$(this).width();
				});
				$this.css({"width": MenuWidth+50+"px"});
				$this.css('visibility', 'hidden');
				$this.show();
				if($this.offset().left+(MenuWidth+50) >= $MainWidth){					
					$this.css({"right":"0", "margin-left":"auto"});	
				}
				$this.hide();
				$this.css('visibility', 'visible');
			}); 
		}
		// Added for RUE-2869 Caraousle Issue on TAB
		resNavigation();
		$(window).on('resize', function(){
			resNavigation();
		});
		
		/* MeganNav active Tab functionality */		
		(function(){
			var activeDept= window.activeDept || "";
			if(activeDept!=="" && typeof(activeDept)!=="undefined"){
				activeDept= activeDept.replace(/\s/g, '').toLowerCase();
				$(".primary-nav>ul>li>a").each(function(){
					if($(this).prop('id')==""+activeDept+"" || $(this).text().toLowerCase()==""+activeDept+"") {
						$(this).addClass("selected");
						return false;
					}
				});
			}			
		})();
	})
  </script>
